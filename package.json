{
  "name": "imaginary-seychelles-bank-system",
  "version": "1.0.0",
  "description": "Web system for the Imaginary Seychelles Bank",
  "scripts": {
    "prepare": "husky",
    "test": "pnpm -r test"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
