    "prettier-plugin-sql": "^0.18.1"
  },
  "packageManager": "pnpm@9.11.0",
  "lint-staged": {
    "*.{md,sql}": "prettier --write"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-sql"
    ],
    "language": "mysql",
    "keywordCase": "upper"
  },
  "engines": {
    "node": ">=22.9.0",
    "pnpm": ">=9.11.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  }
